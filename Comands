docker-compose -f stack.yml up -d
docker ps
docker exec -it 7b44ba378e7f /bin/bash
docker-compose -f stack.yml up -d
docker-compose -f stack.yml down

Установка докера
apt update
apt upgrade
sudo apt install curl software-properties-common ca-certificates apt-transport-https -y
wget -0- https://download.docker.com/linux/ubuntu/gpg | gpg --dearmor | sudo tee /etc/apt/keyrings/docker.gpg > /dev/null
echo "deb [arch=amd64 signed-by=/etc/apt/keyrings/docker.gpg] https://download.docker.com/linux/ubuntu jammy stable"| sudo tee /etc/apt/sources.list.d/docker.list > /dev/null
apt update
apt-cache policy docker-ce
sudo apt install docker-ce -y
sudo systemctl status docker

docker-compose up
curl -L "https://github.com/docket/compose/releases/download/1.29.2/docker-compose-$(uname -s)" -o /usr/local/bin/docker-compose
chmod +x /usr/local/bin/docker-compose
docker-compose --version
usermod -aG docker $USER
newgrp docker

apt install apache2
apache2-v
docker-compose -f stack.yml up -d
apt install php
docker-compose -f stack.yml down
docker-compose -f stack.yml up -d
docker ps 
docker exec -it ИМЯ КОНТЕЙНЕРА /bin/bash
apt install ifupdown
nano LocalSettings.php поменять локалхост и изменить айпи по необходимости
